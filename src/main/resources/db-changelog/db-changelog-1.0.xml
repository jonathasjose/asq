<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

 	<changeSet  id="1"  author="Jonathas J. C.">  
         <createTable  tableName="procedimentos">  
             <column name="id" type="serial">  
                 <constraints  primaryKey="true"  nullable="false" />  
             </column>  
             <column name="codigo" type="int" /> 
             <column name="idade" type="int" /> 
             <column name="tp_sexo" type="varchar(10)" /> 
             <column name="fg_permitido" type="boolean" defaultValueBoolean="false"/>  
         </createTable>  
    </changeSet>
    
    
</databaseChangeLog>